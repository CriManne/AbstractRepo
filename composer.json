{
    "name": "crimanne/abstract-repo",
    "description": "A small light-weight library to abstract repositories and avoid writing tons of repository logic.",
    "version": "0.1.0",
    "autoload": {
        "psr-4": {            
            "AbstractRepo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AbstractRepo\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Cristian Mannella",
            "email": "mannellacristian@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    },
    "scripts": {
        "test":"./vendor/bin/phpunit tests"
    }
}
